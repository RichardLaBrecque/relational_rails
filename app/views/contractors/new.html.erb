<form action="/contractors" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter a new Contractor:</p>
  <input type='text' name='contractor[name]' value='contractor name'/><br/>
  <input type='text' name='contractor[licensed]' value='licensed?'><br/>
  <input type='text' name='contractor[service_cost]' value=0><br/>
  <input type='submit'/>
</form>
